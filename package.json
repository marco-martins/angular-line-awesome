{
  "name": "angular-line-awesome",
  "description": "Simple, easy to use Angular component to manage Line Awesome icons.",
  "version": "19.0.0",
  "author": "Marco Martins",
  "license": "MIT License",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marco-martins/angular-line-awesome.git"
  },
  "keywords": [
    "Angular",
    "Line Awesome",
    "Icons",
    "Icons 8",
    "SVG"
  ],
  "engines": {
    "node": "~v20.x.x",
    "npm": "~9.x.x"
  },
  "bugs": {
    "url": "https://github.com/marco-martins/angular-line-awesome/issues"
  },
  "homepage": "https://github.com/marco-martins/angular-line-awesome#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postbuild": "node copy-dist-files",
    "generate-icons": "svg-to-ts",
    "postinstall": "ng build --project=angular-line-awesome"
  },
  "svg-to-ts": {
    "srcFiles": [
      "./iconsTemp/**/*.svg"
    ],
    "outputDirectory": "./projects/angular-line-awesome/icons",
    "conversionType": "files",
    "interfaceName": "LineAwesomeIcon",
    "typeName": "lineAwesomeIcon",
    "prefix": "",
    "delimiter": "KEBAB",
    "optimizeForLazyLoading": true,
    "modelFileName": "line-awesome-icon",
    "compileSources": true
  },
  "dependencies": {
    "@angular/animations": "^19.1.4",
    "@angular/common": "^19.1.4",
    "@angular/compiler": "^19.1.4",
    "@angular/core": "^19.1.4",
    "@angular/platform-browser": "^19.1.4",
    "@angular/platform-browser-dynamic": "^19.1.4",
    "express": "^4.21.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular-devkit/build-angular": "^19.1.6",
    "@angular/cli": "^19.1.6",
    "@angular/compiler-cli": "^19.1.4",
    "@angular/language-service": "^19.1.4",
    "@types/jasmine": "~5.1.5",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "^22.13.1",
    "@typescript-eslint/eslint-plugin": "8.23.0",
    "@typescript-eslint/parser": "8.23.0",
    "eslint": "^9.19.0",
    "jasmine-core": "~5.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "line-awesome": "^1.3.0",
    "ncp": "^2.0.0",
    "ng-packagr": "^19.1.2",
    "protractor": "^7.0.0",
    "svg-to-ts": "^12.0.0",
    "ts-node": "~10.9.2",
    "typescript": "~5.7.3"
  }
}